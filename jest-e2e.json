{
    "moduleDirectories": ["node_modules", "src"],
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "./",
    "testEnvironment": "node",
    "testRegex": ".e2e-test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
    
      "@car/(.*)": "<rootDir>/src/modules/car/$1"
    }
  }